{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dev": "npm run emulators",
    "emulators": "firebase emulators:start --only functions",
