    "start": "npm run shell"
  },
  "dependencies": {
    "apollo-server-express": "^2.13.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.1",
    "node-fetch": "^2.6.7",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.1"
  },
  "private": true
}
